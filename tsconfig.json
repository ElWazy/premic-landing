{
  "compilerOptions": {
    "target": "es2016",
    "module": "es2022",
    "rootDir": "./server",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node"
  },
  "exclude": [ "client" ]
}
